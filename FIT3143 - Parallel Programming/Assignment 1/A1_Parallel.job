#!/bin/bash
#SBATCH --job-name=A1_Parallel__job
#SBATCH --time=00:10:00
#SBATCH --mem=16G
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=4
#SBATCH --output=A1_Parallel.%j.out
#SBATCH --partition=defq

if [ ! -f A1_Parallel ]
then
 gcc A1_parallel.c -o A1_Parallel -fopenmp
else
 if [ A1_parallel.c -nt A1_Parallel ]
 then
  gcc A1_parallel.c -o A1_Parallel -fopenmp
 fi
fi

./A1_Parallel

exit 0
